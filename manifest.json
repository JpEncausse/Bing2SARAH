{
   "manifest_version": 2,
   
   "name": "Bing2SARAH",
   "description": "Cortana to SARAH",
   "version": "1.0",
   
   "background": {
      "scripts": [ "Bing2SARAH.js" ]
   },
   
   "icons": { 
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png" 
   },
   
   "permissions": [ 
         "webRequest", 
         "webRequestBlocking", 
         "http://*.bing.com/*", 
         "https://*.bing.com/*" 
   ]
}
